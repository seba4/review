{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es5",
    "paths": {
      "@core/*": ["app/core/*"],
      "@mocks/*": ["app/core/mocks/*"],
      "@constants/*": ["app/core/constants/*"],
      "@guards/*": ["app/core/guards/*"],
      "@interceptors/*": ["app/core/interceptors/*"],
      "@data/*": ["app/data/*"],
      "@schema/*": ["app/data/schema/*"],
      "@services/*": ["app/data/services/*"],
      "@stores/*": ["app/data/stores/*"],
      "@shared/*": ["app/shared/*"],
      "@modules/*": ["app/modules/*"],
      "@admin_modules/*": ["app/modules/admin/*"],
      "@env/*": ["environments/*"],
      "@validators/*": ["core/validators/*"]
    },
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
